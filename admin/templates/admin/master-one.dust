<!doctype html>
<html lang="{lang}">
<head>
		
	<title>{title} Admin</title>
	<!-- CSS -->
	<link rel="stylesheet" type="text/css" href="/css/admin/main{css.suffix}.css" />
	<link rel="shortcut icon" href="/favicon.png">
	<script src="//cdnjs.cloudflare.com/ajax/libs/lodash.js/3.10.1/lodash.min.js"></script>
	<!--<script src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>-->
	<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.13.1/jquery.validate.min.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/backbone.js/1.2.1/backbone-min.js"></script>
	<script>
		if(navigator.userAgent.match(/Trident.*rv:11\./)) {
			document.documentElement.className = 'IE11';
		}
	</script>
</head>

<body>

<div class="container">
	<div class="left">
		<div class="logo"></div>
		<nav class="main">
			<ul>
				{#navigation}
					{>"partials/navigation-item"/}
				{/navigation}
			</ul>
		</nav>
		<a href="/auth/logout" class="logout" target="_self"><span>Logout</span></a>
	</div>

	<main></main>
</div>

	<script>
		var INITIAL_STATE = {INITIAL_STATE|s};
	</script>
	<script src="/js/admin/app{js.suffix}.js"></script>
	
</body>
</html>
